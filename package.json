{
  "name": "Example",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "renew": "./scripts/token.sh",
    "gen:schema": "get-graphql-schema https:// > ./src/data/schema.graphql",
    "relay": "relay-compiler",
    "relay:watch": "relay-compiler --watch",
    "postinstall": "patch-package",
    "pod-install-arch": "cd ios && arch -x86_64 pod install && cd ..",
    "pod-update-arch": "cd ios && arch -x86_64 pod install --repo-update && cd ..",
    "watchman-clear": "watchman watch-del-all",
    "android-clean": "cd android && rm -rf build && ./gradlew clean && cd ..",
    "react-native-full-clean": "watchman watch-del-all && rm -rf node_modules && npm i && cd ios && arch -x86_64 pod install",
    "ios:test": "ENVFILE=.env.test npx react-native run-ios --scheme 'ExampleTestEnv'",
    "ios:test-release": "ENVFILE=.env.test npx react-native run-ios --scheme 'ExampleTestEnv' --configuration Release",
    "ios:dev": "ENVFILE=.env.dev npx react-native run-ios --scheme 'ExampleDevEnv'",
    "ios:dev-release": "ENVFILE=.env.dev npx react-native run-ios --scheme 'ExampleDevEnv' --configuration Release",
    "ios:prod": "ENVFILE=.env.prod npx react-native run-ios --scheme 'Example'",
    "ios:prod-release": "ENVFILE=.env.prod npx react-native run-ios --scheme 'Example' --configuration Release",
    "android:test": "react-native run-android --variant=envTestdebug --appIdSuffix envTest",
    "android:test-release": "react-native run-android --variant=envTestrelease --appIdSuffix envTest",
    "android:dev": "react-native run-android --variant=envDevdebug --appIdSuffix envDev",
    "android:dev-release": "react-native run-android --variant=envDevrelease --appIdSuffix envDev",
    "android:prod": "react-native run-android --variant=envProddebug",
    "android:prod-release": "react-native run-android --variant=envProdrelease",
    "bundle-prod": "cd android && ./gradlew clean && ENVFILE=.env.prod ./gradlew assembleRelease",
    "bundle-test": "cd android && ./gradlew clean && ENVFILE=.env.test ./gradlew assembleEnvTest && ENVFILE=.env.test ./gradlew bundleEnvTest && cd ..",
    "bundle-dev": "cd android && ./gradlew clean && ENVFILE=.env.dev ./gradlew assembleEnvDev && ENVFILE=.env.dev ./gradlew bundleEnvDev && cd .."
  },
  "dependencies": {
    "@backpackapp-io/react-native-toast": "github:quattrorx7/react-native-toast",
    "@hookform/resolvers": "^2.9.8",
    "@notifee/react-native": "^7.6.1",
    "@react-native-async-storage/async-storage": "^1.17.10",
    "@react-native-community/netinfo": "^9.3.7",
    "@react-native-firebase/app": "^17.4.2",
    "@react-native-firebase/messaging": "^17.4.2",
    "@react-native-masked-view/masked-view": "^0.2.8",
    "@react-navigation/drawer": "^6.5.0",
    "@react-navigation/native": "^6.0.13",
    "@react-navigation/native-stack": "^6.9.0",
    "@react-navigation/stack": "^6.3.1",
    "@shopify/flash-list": "^1.4.1",
    "@sumsub/react-native-mobilesdk-module": "^1.23.0",
    "dayjs": "^1.11.6",
    "graphql": "^16.6.0",
    "i18next": "^22.4.6",
    "immer": "^9.0.16",
    "jwt-decode": "^3.1.2",
    "patch-package": "^6.5.0",
    "react": "^18.1.0",
    "react-hook-form": "^7.43.7",
    "react-i18next": "^12.1.1",
    "react-native": "0.70.5",
    "react-native-blob-util": "^0.17.1",
    "react-native-bootsplash": "^4.4.0",
    "react-native-code-push": "^7.1.0",
    "react-native-config": "^1.4.11",
    "react-native-device-info": "^10.3.0",
    "react-native-error-boundary": "^1.1.16",
    "react-native-fast-image": "^8.6.3",
    "react-native-gesture-handler": "^2.8.0",
    "react-native-inappbrowser-reborn": "^3.4.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-linear-gradient": "^2.6.2",
    "react-native-mask-input": "^1.2.2",
    "react-native-modal": "^13.0.1",
    "react-native-modalize": "^2.1.1",
    "react-native-pager-view": "^6.1.2",
    "react-native-pdf": "^6.6.2",
    "react-native-reanimated": "^2.13.0",
    "react-native-safe-area-context": "^4.4.1",
    "react-native-screens": "^3.17.0",
    "react-native-svg": "^13.8.0",
    "react-native-tab-view": "^3.3.3",
    "react-native-vector-icons": "^9.2.0",
    "react-native-webview": "^11.24.0",
    "react-relay": "^14.1.0",
    "relay": "^0.8.0-1",
    "relay-runtime": "^14.1.0",
    "zod": "^3.19.1",
    "zustand": "^4.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^26.0.23",
    "@types/lodash": "^4.14.189",
    "@types/react": "^18.0.21",
    "@types/react-native": "^0.70.0",
    "@types/react-relay": "^14.1.2",
    "@types/react-test-renderer": "^18.0.0",
    "@types/relay-runtime": "^14.1.4",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-relay": "^14.1.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native": "^4.0.0",
    "eslint-plugin-tsdoc": "^0.2.17",
    "get-graphql-schema": "^2.1.2",
    "jest": "^26.6.3",
    "lodash": "^4.17.21",
    "metro-react-native-babel-preset": "^0.72.1",
    "react-native-svg-transformer": "^1.0.0",
    "react-test-renderer": "18.1.0",
    "relay-compiler": "^14.1.0",
    "typescript": "^4.8.3",
    "vscode-apollo-relay": "^1.5.2"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
